<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/txtDali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="sendUrlDali"
        android:text="Axel Dalí Gómez Morales"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="81dp" />

    <Button
        android:id="@+id/txtCota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="sendUrlCota"
        android:text="Daniel Alberto Cota Ochoa"
        app:layout_constraintTop_toBottomOf="@+id/txtDali"
        tools:layout_editor_absoluteX="72dp" />
</androidx.constraintlayout.widget.ConstraintLayout>